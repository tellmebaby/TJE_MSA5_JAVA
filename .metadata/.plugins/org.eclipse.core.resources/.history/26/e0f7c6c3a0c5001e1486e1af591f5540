package _1000;

import java.util.Iterator;

public class _1357 {

	public static void main(String[] args) {
		
		int ln = 2;
		int check1 = 0;
		int check2 = 0;
		
		if(check1 == 0) {
			for (int i = 0; i < 2; i++) {
				System.out.print("1번돌았어요");
				if(check2 == 0) {
					for (int j = 0; j < 2; j++) {
						System.out.print("*");
					}
				}
			}
		}else if (check1 == ln){
			System.out.println(ln+1 +"역전반환점" + check1);
		}
	}
		
}
