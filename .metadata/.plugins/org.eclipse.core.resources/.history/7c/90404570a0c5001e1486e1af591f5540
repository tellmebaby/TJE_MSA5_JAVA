package _1000;

import java.util.Iterator;

public class _1357 {

	public static void main(String[] args) {
		
		int ln = 2;
		int check = 0;
		int lnCheck = 1;
		
		if(check == 0) {
			for (int i = 0; i < 2; i++) {
				System.out.print("*");
			}
		}else if (check == ln){
			System.out.println(ln+1 +"역전반환점" + check);
		}
	}
		
}
