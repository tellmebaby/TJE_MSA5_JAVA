package _1000;

import java.util.Scanner;

/**
		길이 n이 입력되면 다음과 같은 사각형을 출력한다.
		
		예)
		
		n이 5일때
		
		*****
		*   *
		*   *
		*   *
		*****
 */


public class _1356 {

	public static void main(String[] args) {
		// 입력
		
		
		Scanner sc = new Scanner(System.in);
		
		int ln = sc.nextInt();
		
		//처음하고 끝체크
		
		
		// 출력
		
		for (int i = 0; i < ln; i++) {
			if(i == 0 || i == ln-1) {
				for (int j = 0; j < ln; j++) {
					System.out.print("*");
//					System.out.print("t1-1");
				}
			}else {
				for (int j = 0; j < ln; j++) {
					if(j == 0 || j == ln-1) {
						System.out.print("*");
//						System.out.print("t3"+j);
					}else {
						System.out.print(" ");
//						System.out.print("t4"+j);
					}
				}
			}
			System.out.println();
		}
	}
}
